{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch localhost:3000",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "smartStep": true,
            "url": "http://127.0.0.1:3000",
            "sourceMapPathOverrides": {
                "webpack:///./~/*": "${workspaceRoot}/node_modules/*",
                "webpack:///./src/*": "${workspaceRoot}/src/*"
            }
        }
    ]
}
